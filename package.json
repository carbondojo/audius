{
  "name": "audius",
  "productName": "Audius",
  "version": "2.0.0-alpha2",
  "description": "A cross-platform app for downloading songs from YouTube and Spotify",
  "author": {
    "name": "Mihir Pathak",
    "email": "mihirpathak97@gmail.com",
    "url": "https://mihirpathak.in"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/mihirpathak97/audius.git"
  },
  "private": true,
  "main": "src/app.main.js",
  "dependencies": {
    "axios": "^0.19.0",
    "base-64": "^0.1.0",
    "conf": "^5.0.0",
    "dotenv": "6.2.0",
    "electron-is-dev": "^1.1.0",
    "electron-log": "^3.0.6",
    "electron-updater": "^4.1.2",
    "fluent-ffmpeg": "^2.1.2",
    "sanitize-filename": "^1.6.1",
    "ytdl-core": "^0.29.2"
  },
  "devDependencies": {
    "@babel/core": "7.5.5",
    "@fortawesome/fontawesome-svg-core": "^1.2.19",
    "@fortawesome/free-brands-svg-icons": "^5.9.0",
    "@fortawesome/react-fontawesome": "^0.1.4",
    "@svgr/webpack": "4.3.2",
    "@types/jest": "24.0.11",
    "@types/node": "11.11.3",
    "@types/react": "^16.9.1",
    "@types/react-dom": "^16.8.5",
    "@types/react-router": "^5.0.3",
    "@types/react-router-dom": "^4.3.1",
    "@typescript-eslint/eslint-plugin": "1.13.0",
    "@typescript-eslint/parser": "1.13.0",
    "antd": "^3.22.0",
    "babel-eslint": "10.0.2",
    "babel-jest": "^24.8.0",
    "babel-loader": "8.0.6",
    "babel-plugin-module-resolver": "^3.2.0",
    "babel-plugin-named-asset-import": "^0.3.3",
    "babel-preset-react-app": "^9.0.1",
    "camelcase": "^5.2.0",
    "case-sensitive-paths-webpack-plugin": "2.2.0",
    "concurrently": "^4.1.1",
    "cross-env": "^5.2.0",
    "css-loader": "2.1.1",
    "dotenv-expand": "4.2.0",
    "electron": "5.0.7",
    "electron-builder": "21.1.1",
    "eslint": "^6.1.0",
    "eslint-config-react-app": "^5.0.1",
    "eslint-loader": "2.2.1",
    "eslint-plugin-flowtype": "3.13.0",
    "eslint-plugin-import": "2.18.2",
    "eslint-plugin-jsx-a11y": "6.2.3",
    "eslint-plugin-react": "7.14.3",
    "eslint-plugin-react-hooks": "^1.6.1",
    "file-loader": "3.0.1",
    "fs-extra": "7.0.1",
    "html-webpack-plugin": "4.0.0-beta.5",
    "husky": "^3.0.4",
    "identity-obj-proxy": "3.0.0",
    "is-wsl": "^1.1.0",
    "jest": "24.8.0",
    "jest-environment-jsdom-fourteen": "0.1.0",
    "jest-resolve": "24.8.0",
    "jest-watch-typeahead": "0.3.1",
    "less": "^3.10.3",
    "less-loader": "^5.0.0",
    "lint-staged": "^9.2.3",
    "mini-css-extract-plugin": "0.5.0",
    "optimize-css-assets-webpack-plugin": "5.0.3",
    "pnp-webpack-plugin": "1.5.0",
    "postcss-flexbugs-fixes": "4.1.0",
    "postcss-loader": "3.0.0",
    "postcss-normalize": "7.0.1",
    "postcss-preset-env": "6.7.0",
    "postcss-safe-parser": "4.0.1",
    "prettier": "1.18.2",
    "progress-stream": "^2.0.0",
    "query-string": "^6.8.1",
    "react": "^16.9.0",
    "react-app-polyfill": "^1.0.2",
    "react-dev-utils": "^9.0.3",
    "react-dom": "^16.9.0",
    "react-redux": "^7.1.0",
    "react-router": "^5.0.1",
    "react-router-dom": "^5.0.1",
    "redux": "^4.0.4",
    "resolve": "1.12.0",
    "resolve-url-loader": "3.1.0",
    "semver": "6.3.0",
    "style-loader": "1.0.0",
    "styled-components": "^4.3.2",
    "terser-webpack-plugin": "1.4.1",
    "ts-pnp": "1.1.2",
    "typescript": "3.5.3",
    "url-loader": "2.1.0",
    "wait-on": "^3.3.0",
    "webpack": "4.39.1",
    "webpack-dev-server": "3.2.1",
    "webpack-manifest-plugin": "2.0.4",
    "workbox-webpack-plugin": "4.3.1"
  },
  "scripts": {
    "start": "concurrently \"cross-env BROWSER=none node scripts/start.js\" \"wait-on http://localhost:3000 && electron .\"",
    "build": "node scripts/build.js",
    "build-rainbow": "cd bin/rainbow && python3 setup.py build && cd ../",
    "test": "node scripts/test.js --env=jsdom",
    "package:all": "yarn build && electron-builder build -mwl --64x",
    "package:linux": "electron-builder build --linux --x64",
    "package:win": "electron-builder build --win --x64",
    "package:mac": "electron-builder build --mac --x64"
  },
  "lint-staged": {
    "src/**/*.{js,jsx,ts,tsx,json,html,less}": [
      "prettier --single-quote --write",
      "git add"
    ]
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "build": {
    "appId": "in.mihirpathak.audius",
    "copyright": "Copyright Â© 2019 Mihir Pathak",
    "publish": {
      "provider": "github"
    },
    "generateUpdatesFilesForAllChannels": true,
    "files": [
      "react-compiled/**/*",
      "node_modules/**/*",
      "src/app.main.js",
      "src/modules/**/*",
      "package.json"
    ],
    "directories": {
      "buildResources": "assets"
    },
    "extraResources": [
      "bin/rainbow/build"
    ],
    "mac": {
      "target": "dmg",
      "artifactName": "${name}-setup.${ext}"
    },
    "linux": {
      "category": "Audio",
      "synopsis": "A cross-platform app for downloading songs from YouTube and Spotify",
      "target": "AppImage",
      "artifactName": "${name}-${arch}.${ext}"
    },
    "win": {
      "target": "nsis",
      "icon": "build/icon.ico",
      "artifactName": "${productName}Setup-x64.${ext}"
    }
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "jest": {
    "collectCoverageFrom": [
      "src/**/*.{js,jsx,ts,tsx}",
      "!src/**/*.d.ts"
    ],
    "setupFiles": [
      "react-app-polyfill/jsdom"
    ],
    "setupFilesAfterEnv": [],
    "testMatch": [
      "<rootDir>/src/**/__tests__/**/*.{js,jsx,ts,tsx}",
      "<rootDir>/src/**/*.{spec,test}.{js,jsx,ts,tsx}"
    ],
    "testEnvironment": "jest-environment-jsdom-fourteen",
    "transform": {
      "^.+\\.(js|jsx|ts|tsx)$": "<rootDir>/node_modules/babel-jest",
      "^.+\\.css$": "<rootDir>/config/jest/cssTransform.js",
      "^(?!.*\\.(js|jsx|ts|tsx|css|json)$)": "<rootDir>/config/jest/fileTransform.js"
    },
    "transformIgnorePatterns": [
      "[/\\\\]node_modules[/\\\\].+\\.(js|jsx|ts|tsx)$",
      "^.+\\.module\\.(css|less)$"
    ],
    "modulePaths": [],
    "moduleNameMapper": {
      "^react-native$": "react-native-web",
      "^.+\\.module\\.(css|less)$": "identity-obj-proxy"
    },
    "moduleFileExtensions": [
      "web.js",
      "js",
      "web.ts",
      "ts",
      "web.tsx",
      "tsx",
      "json",
      "web.jsx",
      "jsx",
      "node"
    ],
    "watchPlugins": [
      "jest-watch-typeahead/filename",
      "jest-watch-typeahead/testname"
    ]
  }
}
